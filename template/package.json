{
  "name": "{{slugcase name}}",
  "productName": "{{capitalcase name}} plugin",
  "version": "0.0.1",
  "description": "{{description}}",
  "main": "lib/index.js",
  "scripts": {
    "compile": "webpack --config ./config/webpack.prod.config.js",
    "start": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublish": "npm run compile",
    "storybook": "start-storybook -p 9001",
    "test": "mocha-webpack \"./src/**/*.spec.js\"",
    "test:watch": "mocha-webpack \"./src/**/*.spec.js\" --watch",
    "cover": "cross-env NODE_ENV=test nyc npm run test"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
