{
  "name": "{{slugcase name}}",
  "productName": "{{capitalcase name}} plugin",
  "version": "0.0.1",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "ci": "npm run check && npm test",
    "fmt": "mongodb-js-fmt ./*.js ./test/*.js",
    "check": "mongodb-js-precommit './src/**/*{.js,.jsx}' './test/**/*.js' index.js",
    "compile": "npm run jsx && npm run less",
    "postcompile": "cp -r styles/fonts lib/styles/",
    "jsx": "babel ./src --out-dir ./lib --presets react",
    "preless": "lessc ./styles/compass/index.less ./lib/styles/compass/index.css",
    "less": "lessc ./styles/index.less ./lib/styles/index.css",
    "link-plugin": "./scripts/link.sh",
    "unlink-plugin": "./scripts/unlink.sh",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "prestart": "npm run compile",
    "start": "electron ./electron",
    "storybook": "start-storybook -p 9001",
    "test": "hadron-build test",
    "test-enzyme": "npm test -- --enzyme",
    "test-main": "npm test -- --main",
    "test-renderer": "npm test -- --renderer",
    "test-unit": "npm test -- --unit",
    "test-functional": "npm test -- --functional",
    "start:dev": "webpack-dev-server --config ./config/webpack.dev.config.js",
    "start:prod": "webpack --config ./config/webpack.prod.config.js && electron ./electron --noDevServer .",
    "build": "webpack --config webpack.prod.config.js"
  },
  "precommit": [
    "check"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.3.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.9.0",
    "babili-webpack-plugin": "^0.0.11",
    "bootstrap": "https://github.com/twbs/bootstrap/archive/v3.3.5.tar.gz",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.5.0",
    "css-loader": "^0.28.1",
    "electron": "^1.6.1",
    "electron-packager": "^8.7.0",
    "electron-mocha": "^3.3.0",
    "enzyme": "^2.4.1",
    "eslint-config-mongodb-js": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "hadron-app": "^1.0.0",
    "hadron-app-registry": "^5.2.0",
    "hadron-build": "^9.1.0",
    "hadron-react-components": "^1.9.1",
    "hadron-spectron": "^0.0.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "mongodb-data-service": "^3.0.0",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-fontawesome": "^1.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.17.0",
    "watch": "^1.0.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
